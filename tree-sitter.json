{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "json",
      "camelcase": "Json",
      "title": "Json",
      "scope": "source.json",
      "file-types": ["json"],
      "injection-regex": "^json$",
      "class-name": "TreeSitterJson",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Json grammar for tree-sitter",
    "authors": [
      {
        "name": "elginas"
      }
    ],
    "links": {
      "repository": "https://github.com/elginas/tree-sitter-json"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
